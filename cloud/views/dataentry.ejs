<% include header.ejs %>


        <!-- Content Header -->
        <div id="content-header">
          <h1>Enter Data</h1>
        </div>
        <!-- Primary Content Container -->
        <div id="content-container">

        <select id="metricSelect" name="metricSelect" class="form-control parsley-validated" data-required="true">

                    <%  
                    for(var i = 0; i < metric_bins.length; i++){ %>
                        <option> <%= metric_bins[i].get("Metric")%> </option>
                   <% } %>
          
        </select>

        <select id="dateSelect" name="dateSelect" class="form-control parsley-validated" data-required="true">

                    <%  
                    for(var j = 0; j < dates.length; j++){ %>
                        <option> <%= dates[j]%> </option>
                   <% } %>
          
        </select>

        <div class="row">
          <div class="col-md-4">
              <h5>Overall</h5>
          </div>        
          <div class="col-md-4">
              <h5>In Queue</h5>
          </div>
        </div>  


<form id="metricForm" method="post" action="/addMetric">


  <div class="form-group">
          <div class="row">

              <label class="col-md-2">Received</label>

              <div class="col-md-2">
                <input id="Received" type="text" class="form-control" placeholder="(Number)">
              </div>
 
 <!-- 
            <label class="col-md-2" id="Bin1Label"> <%= metric_bins[1].attributes.Bin1 %> </label>
   -->             
            <label class="col-md-2" id="Bin1Label"> Testing </label>

              <div class="col-md-2">
                <input type="text" class="form-control" placeholder="(Number)">
              </div>
            </div>
          <div class="row">

              <label class="col-md-2">Completed</label>

              <div class="col-md-2">
                <input type="text" class="form-control" placeholder="(Number)">
              </div>
 
              <label class="col-md-2" id="Bin2Label">1 Day to 5 Days<%/* metric_bins[i].attributes.Bin2 */ %></label>  

              <div class="col-md-2">
                <input type="text" class="form-control" placeholder="(Number)">
              </div>
            </div>
          <div class="row">

              <div class="col-md-4">
              </div>
 
              <label class="col-md-2" id="Bin3Label">More Than 5 Days <%/* metric_bins[i].attributes.Bin3 */ %> </label> 
   

              <div class="col-md-2">
                <input type="text" class="form-control" placeholder="(Number)">
              </div>
            </div>            

            <div>
                <button type="submit" >Add Metric</button>


                </div>

            </div>


         <!-- </div> -->

  </div>
</form>

