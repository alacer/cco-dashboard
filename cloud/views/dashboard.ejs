<% include header.ejs %>



        <!-- Content Header -->
      <div id="content-header">
        <div class="row">
          <div class="col-sm-8">
          <h1>Dashboard Overview</h1>
        </div>
      <div class="col-sm-3" style="margin-top: 15px;">
                  <h4>Date</h4>
                    <div id="dp-ex-3" class="input-group date" data-auto-close="true" data-date-format="yyyy-mm-dd" data-date-autoclose="true" placeholder="Date">
                      <input id="date_input" class="form-control" type="text">
                      <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                    </div>
                    <br>
                </div>
        </div>
      </div>
        <!-- Primary Content Container -->
        <div id="content-container">
          <!-- Alerts Tables Navigation -->
                <div class="clear"></div>
          <!-- Alerts Tables (Open, Submitted, Closed) -->
          <div id="DashboardTabsContent" class="tab-content">
            <!-- Open Alerts -->
            <div class="tab-pane fade in active" id="Overview">
           
<!--
              <div class="form-group">  
                  <label for="validateSelect">Date</label>
                  <select id="validateSelect" name="validateSelect" class="form-control parsley-validated" data-required="true">

                    <%  
                    for(var j = 0; j < uniquedates.length; j++){ %>
                        <option> <%= uniquedates[j]%> </option>
                   <% } %>  

                  </select>
              </div>
--> 

              <div class="table-responsive">

                <table id=#metricsdt class="table table-striped table-bordered table-hover table-highlight table-checkable" data-provide="datatable" data-display-rows="15" data-info="false" data-search="false" data-length-change="false" data-paginate="true">
                  <thead>
                    <tr>
                      <!--
                      <th id = "Date" data-filterable="true" data-sortable="false" width = "0%">Date</th>
                      -->
                      <th data-filterable="true" data-sortable="true" data-direction="asc" width = "50%">Metric</th>
                      <th data-filterable="false" data-sortable="false" width = "10%">Received</th>
                      <th data-filterable="false" data-sortable="false" width = "10%">Completed</th>
                      <th data-filterable="false" data-sortable="false" width = "20%">In Queue</th>
                      <th data-filterable="false" data-sortable="false" width = "10%">Comments</th>
                    </tr>
                  </thead>

                  <% for(var i = 0; i < metrics.length; i++) { %>
                    <tr data-objectid="<%= metrics[i].attributes.Metric %>">
                        <!--
                        <td> <%= metrics[i].attributes.Date %> </td>
                        -->
                        <td> <%= metrics[i].attributes.Metric %> </td>
                        <td> <%= metrics[i].attributes.Received %> </td>
                        <td> <%= metrics[i].attributes.Completed %> </td>
                        <td>
                          <a href="" class="ui-popover" data-toggle="tooltip" data-placement="left" data-trigger="hover" data-html=TRUE data-content="<b> <%= metric_bins[i].attributes.Bin1 %>: </b><%= metrics[i].attributes.OK %> <br/> <b> <%= metric_bins[i].attributes.Bin2 %>: </b> <%= metrics[i].attributes.Caution %> <br/> <b> <%= metric_bins[i].attributes.Bin3 %>: </b> <%= metrics[i].attributes.Danger %>">
                          <div class="progress">
                            <div class="progress-bar progress-bar-success" style="width: <%=percentok[i]%>%">
                            </div>
                            <div class="progress-bar progress-bar-warning progress-bar-striped" style="width: <%=percentcaution[i]%>%; background-color: #FFF000">
                            </div>
                            <div class="progress-bar progress-bar-danger" style="width: <%=percentwarning[i]%>%">
                            </div>
                          </div>
                          </a>
                        </td>
                  <!--     <td> <%= metrics[i].attributes.InQueue %> </td> -->

                        <td><a href="" class="ui-popover" data-toggle="tooltip" data-placement="left" data-trigger="hover" title="Comments" data-content=<%= metrics[i].attributes.Comments ? metrics[i].attributes.Comments : 'None' %> >
                          <i class="fa fa-comments-o text-xl"></i>
                        </a></td>
                       <!-- <td> <%= metrics[i].attributes.Comments %> </td>  -->                         
                      </tr>
                    <% } %> 
                    

                </table>
              </div>



          
          <!-- /#AlertsTabsContent -->
          <!-- Investigator Status -->
        </div>
      </div>
    </div>

<% include footer.ejs %>