<% include header.ejs %>

        <!-- Content Header -->
        <div id="content-header">
          <h1>Performance Overview</h1>
        </div>
        <!-- Primary Content Container -->
        <div id="content-container">
          <!-- Alerts Tables Navigation -->

          <!-- Alerts Tables (Open, Submitted, Closed) -->
          <div id="AlertsTabsContent" class="tab-content">
            <!-- Open Alerts -->
            <div class="tab-pane fade in active" id="OpenAlerts">

              <div class="form-group">  
                  <label for="validateSelect">Date</label>
                  <select id="validateSelect" name="validateSelect" class="form-control parsley-validated" data-required="true">






                    <%  
                    for(var j = 0; j < uniquedates.length; j++){ %>
                        <option> <%= uniquedates[j]%> </option>
                   <% } %>



                     

                  </select>
              </div>


              <div class="table-responsive">
                <table class="table table-striped table-bordered table-hover table-highlight table-checkable" data-provide="datatable" data-display-rows="20" data-info="false" data-search="false" data-length-change="true" data-paginate="true">
                  <thead>
                    <tr>
                      <th data-filterable="true" data-sortable="true" data-direction="asc">Metric</th>
                      <th data-filterable="false" data-sortable="false">Received</th>
                      <th data-filterable="false" data-sortable="false">Completed</th>
                      <th data-filterable="false" data-sortable="false">In Queue</th>
                      <th data-filterable="false" data-sortable="false">Comments</th>
                    </tr>
                  </thead>

                  <% for(var i = 0; i < metrics.length; i++) { %>
                    <tr data-objectid="<%= metrics[i].id %>">
                        <td> <%= metrics[i].attributes.Metric %> </td>
                        <td> <%= metrics[i].attributes.Received %> </td>
                        <td> <%= metrics[i].attributes.Completed %> </td>
                        <td>
                          <div class="progress">
                            <div class="progress-bar progress-bar-success" style="width: 75%">
                            </div>
                            <div class="progress-bar progress-bar-warning progress-bar-striped" style="width: 20%; background-color: #FFF000">
                            </div>
                            <div class="progress-bar progress-bar-danger" style="width: 5%">
                            </div>
                          </div>
                        </td>
                  <!--     <td> <%= metrics[i].attributes.InQueue %> </td> -->

                        <td><a href="" class="ui-popover" data-toggle="tooltip" data-placement="left" data-trigger="hover" data-content=<%= metrics[i].attributes.Comments %> ><i class="fa fa-comments-o text-xl"></i></a></td>
                       <!-- <td> <%= metrics[i].attributes.Comments %> </td>  -->                         
                      </tr>
                    <% } %> 
                    

                </table>
              </div>

          
          <!-- /#AlertsTabsContent -->
          <!-- Investigator Status -->
        </div>
      </div>
    </div>

<% include footer.ejs %>