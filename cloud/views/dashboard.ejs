<% include header.ejs %>

        <!-- Content Header -->
        <div id="content-header">
          <h1>Performance Overview</h1>
        </div>
        <!-- Primary Content Container -->
        <div id="content-container">
          <!-- Alerts Tables Navigation -->

          <!-- Alerts Tables (Open, Submitted, Closed) -->
          <div id="AlertsTabsContent" class="tab-content">
            <!-- Open Alerts -->
            <div class="tab-pane fade in active" id="OpenAlerts">
              <div class="table-responsive">
                <table id="dashboardtable" class="table table-striped table-bordered table-hover table-highlight table-checkable" data-provide="datatable" data-display-rows="20" data-info="false" data-search="false" data-length-change="true" data-paginate="false">
                  <thead>
                    <tr>
                      <th data-filterable="true" data-sortable="true" data-direction="asc">Metric</th>
                      <th data-filterable="false" data-sortable="false">Received</th>
                      <th data-filterable="false" data-sortable="false">Completed</th>
                      <th data-filterable="false" data-sortable="false">In Queue</th>
                      <th data-filterable="false" data-sortable="false">Comments</th>
                    </tr>
                  </thead>
                  
                </table>
              </div>

          
          <!-- /#AlertsTabsContent -->
          <!-- Investigator Status -->
        </div>
      </div>
    </div>

